public Boolean companionPlants(String plant1, String plant2) {
    //code here
    
    Map<String, String> plants = new Map<String, String>{
        'cucumbers' => 'lettuce',
        'onions' => 'lettuce',
        'carrots' => 'onions',
        'tomatoes' => 'onions'
    };

    if(plants.get(plant1) == plant2 || plants.get(plant2) == plant1){
        return true;
    } else
        return false;
}